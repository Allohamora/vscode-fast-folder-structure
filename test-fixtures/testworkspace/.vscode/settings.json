    "Indexfile Import/Export": [
      "import <FFSName> from './<FFSName>'",
      "",
      "export default <FFSName>;"
    ],
    "Multiple Variable Test": [
      "fillerText1",
      "names <FFSName> inside <testVar> string",
      "",
      "<FFSName> no transformation",
      "<FFSName | uppercase> uppercase",
      "<FFSName | capitalize> capitalized",
      "<FFSName | lowercase> lowercase",
      "",
      "fillerText2",
      "fillerText3",
