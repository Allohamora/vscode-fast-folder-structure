      "<testVar2 | capitalize> capitalized",
      "<testVar2 | lowercase> lowercase",
      "",
      "fillerText6",
      "fillerText7"
    ]
  },
  "fastFolderStructure.structures": [
    {
      "name": "TS React Functional Component",
      "structure": [
        {
          "fileName": "<FFSName>.tsx",
          "template": "No Custom Variable Test"
        },
        {
          "fileName": "<FFSName>.scss"
        },
        {
          "fileName": "/__tests__/<FFSName>.test.ts"
        },
        {
          "fileName": "index.ts",
          "template": "Indexfile Import/Export"
        }
      ]
    },
    {
      "name": "Multivar Test",
      "customVariables": ["testVar", "testVar2"],
      "structure": [
        {
          "fileName": "<FFSName>.jsx",
          "template": "Multiple Variable Test"
        },
        {
          "fileName": "uppercasefilename <FFSName | uppercase>.jsx"
        },
        {
          "fileName": "lowercase customVariable in Filename <testVar | lowercase>.scss"
        },
        {
          "fileName": "/nestedFolder/<FFSName> <testVar> <testVar2> Chain Variables.txt"
        },
        {
          "fileName": "/nestedFolder/nestedFolder2/<FFSName>.test.js"
        },
        {
          "fileName": "index.js",
          "template": "Indexfile Import/Export"
        },
        {
          "fileName": "EmptyDirectory",
