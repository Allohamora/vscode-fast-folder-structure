      "<testVar> no transformation",
      "<testVar | uppercase> uppercase",
      "<testVar | capitalize> capitalized",
      "<testVar | lowercase> lowercase",
      "",
      "fillerText4",
      "fillerText5",
      "",
      "<testVar2> no transformation",
